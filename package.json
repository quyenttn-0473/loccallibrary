{
    "name": "book-locallibrary",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "build": "tsc",
        "start": "node ./bin/www",
        "dev:start": "nodemon ./src/index.ts",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
        "migration:start": "npm run typeorm -- -d ./src/data-source.ts migration:generate ./src/config/migrations/InitMigration",
        "migration:revert": "npm run typeorm migration:revert -- -d ./src/data-source.ts ",
        "migration:run": "npm run typeorm migration:run -- -d ./src/data-source.ts"
    },
    "dependencies": {
        "@types/redis": "^4.0.11",
        "connect-flash": "^0.1.1",
        "connect-redis": "^7.1.1",
        "cookie-parser": "~1.4.4",
        "debug": "~2.6.9",
        "dotenv": "^16.4.5",
        "express": "~4.16.1",
        "express-session": "^1.18.0",
        "http-errors": "~1.6.3",
        "method-override": "^3.0.0",
        "morgan": "~1.9.1",
        "mysql2": "^3.10.2",
        "pug": "2.0.0-beta11",
        "redis": "^4.6.15",
        "reflect-metadata": "^0.2.2",
        "tsconfig-paths": "^4.2.0",
        "typedi": "^0.10.0",
        "typeorm": "^0.3.20"
    },
    "devDependencies": {
        "@types/connect-flash": "^0.0.40",
        "@types/cookie-parser": "^1.4.7",
        "@types/express": "^4.17.21",
        "@types/express-session": "^1.18.0",
        "@types/method-override": "^0.0.35",
        "@types/morgan": "^1.9.9",
        "@types/node": "^20.14.10",
        "nodemon": "^3.1.4",
        "ts-node": "^10.9.2",
        "typescript": "^5.5.3"
    }
}
